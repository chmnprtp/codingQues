
#include <iostream>
using namespace std;

int main() {
    
    int n;
    int count=0;
    cout << "enter number to check prime";
    cin >> n;
    
     if(n==1)
        {
        cout<<"number is not prime";
        return 0;
        }
   
    for(int i=2; i*i<=n; i++)
    {
       
    
        if(n%i==0)
        {
            count++;
        }
    }
    
    if(count)
    {
        cout<<"number is not prime";
    }
    else{
        cout<<"number is prime";
    }
    return 0;
}
